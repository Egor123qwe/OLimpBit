nt(\"li\", {\n    __self: _this,\n    __source: {\n      fileName: _jsxFileName,\n      lineNumber: 15,\n      columnNumber: 21\n    }\n  }, \"\\u0421\\u043E\\u0434\\u0435\\u0439\\u0441\\u0442\\u0432\\u0438\\u0435 \\u043F\\u043E\\u0432\\u044B\\u0448\\u0435\\u043D\\u0438\\u044E \\u044D\\u0444\\u0444\\u0435\\u043A\\u0442\\u0438\\u0432\\u043D\\u043E\\u0441\\u0442\\u0438 \\u0438 \\u0440\\u0430\\u0441\\u0448\\u0438\\u0440\\u0435\\u043D\\u0438\\u044E \\u0443\\u0447\\u0430\\u0441\\u0442\\u0438\\u044F \\u0411\\u0413\\u0423\\u0418\\u0420 \\u0432 \\u0441\\u0438\\u0441\\u0442\\u0435\\u043C\\u043E\\u0442\\u0435\\u0445\\u043D\\u0438\\u0447\\u0435\\u0441\\u043A\\u0438\\u0445 \\u0438 \\u0438\\u043D\\u043D\\u043E\\u0432\\u0430\\u0446\\u0438\\u043E\\u043D\\u043D\\u044B\\u0445 \\u043F\\u0440\\u043E\\u0435\\u043A\\u0442\\u0430\\u0445 \\u0441\\u043E\\u043E\\u0442\\u0432\\u0435\\u0442\\u0441\\u0442\\u0432\\u0443\\u044E\\u0449\\u0435\\u0439 \\u043D\\u0430\\u043F\\u0440\\u0430\\u0432\\u043B\\u0435\\u043D\\u043D\\u043E\\u0441\\u0442\\u0438 \\u0434\\u043B\\u044F \\u0448\\u0438\\u0440\\u043E\\u043A\\u043E\\u0433\\u043E \\u043A\\u0440\\u0443\\u0433\\u0430 \\u0437\\u0430\\u043A\\u0430\\u0437\\u0447\\u0438\\u043A\\u043E\\u0432, \\u0432\\u043A\\u043B\\u044E\\u0447\\u0430\\u044F \\u0437\\u0430\\u0440\\u0443\\u0431\\u0435\\u0436\\u043D\\u044B\\u0445.\"))));\n};\nexport default Ciir;","map":{"version":3,"names":["React","s","Ciir","GeneralText"],"sources":["/Users/igortryhan/BSUIR/Sovet/WebProject/FrontEnd/proj/OLimpBit/src/components/Olimpiad/Sections/Sponsors/Sponsor/Ciir/Ciir.jsx"],"sourcesContent":["import React from 'react'\nimport s from './Ciir.module.css';\n\nconst Ciir = () => {\n    return (\n        <div className={s.Ciir}>\n            <h1>Центр информатизации и инновационных разработок</h1>\n            <div className={s.GeneralText}>\n                <p>Центр информатизации и инновационных разработок (ЦИИР) был создан в 2001 году на базе и вместо ранее существовавшего Управления информатизации БГУИР и является структурным подразделением БГУИР. Создание ЦИИР обусловлено необходимостью решения новых задач информатизации, а также в целях обеспечения своевременного и полного выполнения перспективного плана разработки и внедрения информационных технологий в университете.</p>\n                <p><b>Основные направления в деятельности ЦИИР:</b></p>\n                <ul>\n                    <li>Поддержка и координация разработки, внедрения и применения современных информационных систем и информационных технологий в сфере образования.</li>\n                    <li>Разработка, развитие и реализация (совместно с ведущими специалистами БГУИР в области информатизации) Концепции интегрированной информационной системы БГУИР.</li>\n                    <li>Информационно-аналитическое обеспечение учебной, научной, административно-хозяйственной, международной и других видов деятельности БГУИР.</li>\n                    <li>Содействие повышению эффективности и расширению участия БГУИР в системотехнических и инновационных проектах соответствующей направленности для широкого круга заказчиков, включая зарубежных.</li>\n                </ul>\n            </div>\n        </div>\n    );\n}\n\n\nexport default Ciir;"],"mappings":";;AAAA,OAAOA,KAAK,MAAM,OAAO;AACzB,OAAOC,CAAC,MAAM,mBAAmB;AAEjC,IAAMC,IAAI,GAAG,SAAPA,IAAI,GAAS;EACf,oBACI;IAAK,SAAS,EAAED,CAAC,CAACC,IAAK;IAAA;IAAA;MAAA;MAAA;MAAA;IAAA;EAAA,gBACnB;IAAA;IAAA;MAAA;MAAA;MAAA;IAAA;EAAA,4QAAwD,eACxD;IAAK,SAAS,EAAED,CAAC,CAACE,WAAY;IAAA;IAAA;MAAA;MAAA;MAAA;IAAA;EAAA,gBAC1B;IAAA;IAAA;MAAA;MAAA;MAAA;IAAA;EAAA,urEAA2a,eAC3a;IAAA;IAAA;MAAA;MAAA;MAAA;IAAA;EAAA,gBAAG;IAAA;IAAA;MAAA;MAAA;MAAA;IAAA;EAAA,mOAAgD,CAAI,eACvD;IAAA;IAAA;MAAA;MAAA;MAAA;IAAA;EAAA,gBACI;IAAA;IAAA;MAAA;MAAA;MAAA;IAAA;EAAA,+vBAAsJ,eACtJ;IAAA;IAAA;MAAA;MAAA;MAAA;IAAA;EAAA,g1BAAsK,eACtK;IAAA;IAAA;MAAA;MAAA;MAAA;IAAA;EAAA,4uBAAkJ,eAClJ;IAAA;IAAA;MAAA;MAAA;MAAA;IAAA;EAAA,2gCAAsM,CACrM,CACH,CACJ;AAEd,CAAC;AAGD,eAAeD,IAAI"},"metadata":{},"sourceType":"module"